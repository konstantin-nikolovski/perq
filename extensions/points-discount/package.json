{
  "name": "points-discount",
  "private": true,
  "type": "module",
  "dependencies": {
    "@shopify/shopify_function": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4"
  },
  "graphql-codegen": {
    "schema": "./schema.graphql",
    "documents": [
      "src/*.graphql"
    ],
    "generates": {
      "./generated/api.ts": {
        "plugins": [
          "typescript",
          "typescript-operations"
        ]
      }
    }
  },
  "codegen": {
    "schema": "./schema.graphql",
    "documents": [
      "src/*.graphql"
    ],
    "generates": {
      "./generated/api.ts": {
        "plugins": [
          "typescript",
          "typescript-operations"
        ]
      }
    }
  }
}
